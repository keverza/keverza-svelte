{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true},
		"paths": {
			
			"$stores": ["src/stores"],
			"$stores/*": ["src/stores/*"],
			"$components": ["src/components"],
			"$components/*": ["src/components/*"],
			"$api": ["src/routes/api"],				
			"$api/*": ["src/routes/api/*"],				
			"$styles": ["src/styles"],
			"$styles/*": ["src/styles/*"],
		},
	
}
